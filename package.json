{
  "name": "qr-code-generator",
  "displayName": "QRCode Generator",
  "description": "Generate QR code in VS Code",
  "version": "1.2.0",
  "publisher": "coderwsh",
  "icon": "assets/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/coderwang/qr-code-generator"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.generateQRCode",
        "title": "Generate QR Code"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "resourcePath =~ /\\/src\\/pages\\// && !(resourcePath =~ /\\/components\\//) && resourceExtname == .tsx",
          "command": "extension.generateQRCode",
          "group": "navigation"
        }
      ],
      "explorer/context": [
        {
          "when": "resourcePath =~ /\\/src\\/pages\\// && !(resourcePath =~ /\\/components\\//) && resourceExtname == .tsx",
          "command": "extension.generateQRCode",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "20.2.5",
    "@types/qrcode": "^1.5.1",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "qrcode": "^1.5.3"
  }
}