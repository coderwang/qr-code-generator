{
  "name": "qr-code-generator",
  "displayName": "QRCode Generator",
  "description": "Generate QR code in VS Code",
  "keywords": [
    "qrcode",
    "qr",
    "二维码",
    "二維碼",
    "erweima"
  ],
  "version": "2.1.2",
  "publisher": "coderwsh",
  "icon": "assets/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/coderwang/qr-code-generator"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "QRCode Generator",
      "properties": {
        "QRCodeGenerator.DefaultQRCodeUrl": {
          "type": "string",
          "default": "https://www.google.com",
          "description": "Default QRCode URL, it can be empty."
        }
      }
    },
    "commands": [
      {
        "command": "extension.generateQRCode",
        "title": "Generate QR Code"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.generateQRCode",
          "group": "navigation",
          "when": "editorHasSelection"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "20.2.5",
    "@types/qrcode": "^1.5.1",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "glob": "^10.3.3",
    "qrcode": "^1.5.3"
  }
}
